{% extends '::base.html.twig' %}
{% block meta_keywords %}{{ page.translation.metaKeywords }}{% endblock meta_keywords %}
{% block meta_description %}{{ page.translation.metaDescription }}{% endblock meta_description %}
{% block title %}{{ page.translation.title }}{% endblock %}
{% block brand %} - INSERT PROJECT NAME HERE{% endblock brand %}
{% block css %}{{ page.css|raw }}{% endblock css %}
{% block js %}{{ page.js|raw }}{% endblock js %}
{% block page_content %}
{% if block('content')|length > 0 %}
    {{ block('content') }}
{% else %}
    {{ msi_block_render_block('content', page) }}
{% endif %}
{% endblock page_content %}
